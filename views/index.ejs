<% include header %>

<!-- Main jumbotron for a primary marketing message or call to action -->
<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <form>
      <div class="form-group">
        <input type="text" class="form-control" id="formGroupExampleInput" placeholder="Introduce keywords or location">
      </div>
      <div class="form-group">
        <input class="btn btn-primary" type="submit">
      </div>
    </form>
  </div>
</div>

<div class="container">
  <!-- Example row of columns -->

  <div class="row">
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <div class="col-md-6">
      <div class="card">
        <div class="card-body">
          <h6>RestaurantName</h6>
          <p class="card-text">★★★★☆ 4/5</p>
          <a href="restaurant" class="card-link">ViewRestaurant</a>
          <br>
        </div>
      </div>
    </div>
  </div>

  <hr>

</div> <!-- /container -->

<% include footer %>
        <div class="text-center">
            <h3><%= if(0)(restaurant[0].rating.totalScore/restaurant[0].reviews.length).toFixed(2) %>/5</h3>
            <% for(i=0; i<Math.round(restaurant[0].rating.totalScore/restaurant[0].reviews.length); i++) {%>
                <i class="fas fa-star"></i>
            <%} %>
            <% for(i=0; i<(5-Math.round(restaurant[0].rating.totalScore/restaurant[0].reviews.length)); i++) {%>
                <i class="far fa-star"></i>
            <%} %>
        </div>


